{
  ".flBlock": {
    "flexProps": {
      "alignContent": "STRETCH",
      "alignItems": "STRETCH",
      "justifyContent": "CENTER"
    },
    "&-imgBlock": {
      "imageFillType": "ASPECTFILL",
      "flexProps": {
        "positionType": "ABSOLUTE"
      },
      "left": 0,
      "right": 0,
      "bottom": 0,
      "top": 0
    },
    "&-imgCrack": {
      "imageFillType": "ASPECTFILL",
      "flexProps": {
        "positionType": "ABSOLUTE"
      },
      "left": 0,
      "right": 0,
      "bottom": 0,
      "top": 0,
      "alpha": 0.8,
      "visible": false
    },
    "backgroundColor": "rgba( 192, 8, 8, 1 )",
    "borderColor": "rgba( 0, 0, 0, 1 )",
    "borderWidth": 1
  }
}